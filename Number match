n=int(input())
arr=list(map(int,input().split()))
a,b=0,0
for num in set(arr):
    if num%2==0:
       if arr.count(num)%2==0:
          a+=1
      else:
          b+=1
    else:
        if arr.count(num)%2==0
           b+=1
        else:
            a+=1
if a>b:
      print("A,a-b")
elif ab:
      print("T 0")
else:
    print("B,b-a")|
